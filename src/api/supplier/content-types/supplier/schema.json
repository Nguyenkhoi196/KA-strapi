{
  "kind": "collectionType",
  "collectionName": "suppliers",
  "info": {
    "singularName": "supplier",
    "pluralName": "suppliers",
    "displayName": "supplier",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "boolean",
      "default": true,
      "private": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "loyalty": {
      "type": "enumeration",
      "enum": [
        "low",
        "medium",
        "high"
      ]
    },
    "Info": {
      "type": "dynamiczone",
      "components": [
        "information.contacts",
        "information.location"
      ],
      "required": true
    }
  }
}
