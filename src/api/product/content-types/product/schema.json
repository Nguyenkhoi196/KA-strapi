{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true,
    "privateAttributes": ["publishedAt"]
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "inventory": {
      "type": "integer",
      "required": false
    },
    "price": {
      "type": "integer"
    },
    "brand": {
      "type": "string"
    },
    "supplier": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::supplier.supplier"
    },
    "state": {
      "type": "boolean",
      "default": true
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    }
  }
}
