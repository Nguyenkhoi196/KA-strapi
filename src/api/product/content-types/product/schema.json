{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "inventory": {
      "type": "integer"
    },
    "price": {
      "type": "integer"
    },
    "brand": {
      "type": "string"
    },
    "supplier": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::supplier.supplier"
    },
    "state": {
      "type": "boolean",
      "default": true
    },
    "img": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
